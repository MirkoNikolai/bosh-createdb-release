crate_endpoints:
- host: <%= p('crate_adapter.dbhost','localhost') %> # Host to connect to (default: "localhost").
  port: <%= p('crate_adapter.dbport','5432') %>      # Port to connect to (default: 5432).
  user: <%= p('crate_adapter.dbuser','crate') %>     # Username to use (default: "crate")
  password: <%= p('crate_adapter.dbpasswd','mypass') %> # Password to use (default: "").
  schema: <%= p('crate_adapter.dbschema','prometheus') %> # Schema to use (default: "").
  max_connections: <%= p('crate_adapter.max_connections','5')%> # The maximum number of concurrent connections (default: 5).
  enable_tls: <%= p('crate_adapter.enable_tls','false')%> # Whether to connect using TLS (default: false).
  allow_insecure_tls: <%= p('crate_adapter.allow_insecure_tls','false')%> # Whether to allow insecure / invalid TLS certificates (default: false).